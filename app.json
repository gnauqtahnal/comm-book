{
  "expo": {
    "name": "comm-book",
    "slug": "comm-book",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./src/assets/logo.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo.jpg",
        "backgroundColor": "#ffffff"
      }
    },
    "expo": {
      "plugins": [
        [
          "expo-image-picker",
          {
            "photosPermission": "Allow app to access your photos.",
            "cameraPermission": "Allow app to access your camera.",
            "microphonePermission": "Allow app to access your microphone."
          }
        ],
        [
          "expo-av",
          {
            "microphonePermission": "Allow app to access your microphone."
          }
        ]
      ]
    }
  }
}
